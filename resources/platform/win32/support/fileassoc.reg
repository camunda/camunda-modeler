Windows Registry Editor Version 5.00

[HKEY_CURRENT_USER\Software\Classes]

[HKEY_CURRENT_USER\Software\Classes\.bpmn]
@="CamundaModeler.BPMN"

[HKEY_CURRENT_USER\Software\Classes\.cmmn]
@="CamundaModeler.CMMN"

[HKEY_CURRENT_USER\Software\Classes\.dmn]
@="CamundaModeler.DMN"

[HKEY_CURRENT_USER\Software\Classes\.form]
@="CamundaModeler.FORM"

[HKEY_CURRENT_USER\Software\Classes\CamundaModeler.BPMN]
@="BPMN file"

[HKEY_CURRENT_USER\Software\Classes\CamundaModeler.BPMN\DefaultIcon]
@="__CWD__,0"

[HKEY_CURRENT_USER\Software\Classes\CamundaModeler.BPMN\shell]
@="open"

[HKEY_CURRENT_USER\Software\Classes\CamundaModeler.BPMN\shell\open]
@="Open with Camunda Modeler"

[HKEY_CURRENT_USER\Software\Classes\CamundaModeler.BPMN\shell\open\command]
@="__CWD__ \"%1\""

